img {
  max-width: 100%;
}
.header-block.text.menu {
  background-color: #fff;
  margin: 0;
  padding: 1rem;
}
.header-block.text.menu .item {
  text-transform: uppercase;
}
.header-block.text.menu .item a {
  text-transform: inherit;
}
.header-block.text.menu .item.logo {
  font-size: 2rem;
  color: #00c0cb;
}
.header-block.text.menu .item.logo img {
  height: 45px;
}
.header-block.text.menu .item.logo a {
  color: inherit;
}
.footer {
  overflow: hidden;
  padding: 3rem;
  background-color: #00848b;
}
.footer .text.menu {
  margin: 0 0 2rem 0;
}
.footer .text.menu .item {
  color: #fff;
}
.footer .text.menu a {
  color: inherit;
}
.footer p {
  margin: 0;
  color: #fff;
}
.main .promo-block {
  overflow: hidden;
  padding: 180px 1rem 17px 1rem;
  background-color: #056165;
}
.main .promo-block .header {
  color: #fff;
}
.main .promo-block .button-box {
  text-align: center;
  color: #fff;
}
.main .promo-block .menu.socials {
  float: right;
}
.main .search-block .search-header {
  margin: 0;
  padding: 0.5rem 1rem;
  background-color: #e9eaed;
}
.main .search-block .search-result-block {
  height: 200px;
  outline: 1px solid red;
}
.main .info-block-1 {
  overflow: hidden;
  background-color: #00848b;
  padding-top: 2rem;
  padding-bottom: 2rem;
  color: #fff;
}
.main .info-block-1 .header {
  color: inherit;
}
.main .info-block-1 .video-box {
  width: 400px;
  height: 300px;
  background-color: #ccc;
  border-radius: 40px;
}
.main .info-block-2 {
  overflow: hidden;
  padding-top: 2rem;
  padding-bottom: 2rem;
}
.main .info-block-3 {
  overflow: hidden;
  padding-top: 2rem;
  padding-bottom: 2rem;
  background-color: #e9eaed;
}
.main .info-block-4 {
  overflow: hidden;
  padding-top: 3rem;
  padding-bottom: 3rem;
}
.main .info-block-4 .column {
  text-align: center;
}
.main .subscribe-block {
  overflow: hidden;
  padding-top: 3rem;
  padding-bottom: 3rem;
  background-color: #fbfbfb;
  border-top: dashed 1px #ccc;
}
.profile {
  background-color: #e9eaed;
}
.profile .under-shadow {
  z-index: 1 !important;
}
.profile .attached.top + .ui.segment,
.profile .attached.top + .ui.segments {
  margin-top: 0 !important;
}
.profile .ui.segment:not(.top) + .attached.bottom,
.profile .ui.segments:not(.top) + .attached.bottom {
  margin-top: -2rem !important;
}
.profile .ui.segments {
  z-index: 2;
  border: none;
  box-shadow: none;
}
.profile .ui.segments.attached {
  margin: 0;
}
.profile .ui.segments > .ui.segment {
  border: none;
  border-radius: 0;
  box-shadow: 0px 3px 6px -1px rgba(0, 0, 0, 0.5);
  margin: 0.5rem 0;
}
.profile .ui.segments > .ui.segment:last-child {
  margin-bottom: 0;
}
.profile .ui.segments > .ui.segment:first-child {
  margin-top: 0;
}
.profile .ui.segment {
  z-index: 2;
  margin: 2rem 0;
  border: none;
  border-radius: 0;
  box-shadow: 0px 3px 6px -1px rgba(0, 0, 0, 0.5);
}
.profile .ui.segment.attached {
  width: 100%;
  max-width: 100%;
}
.profile .ui.segment.attached.top {
  margin-bottom: 0;
}
.profile .ui.segment.attached.bottom {
  margin-top: 0;
}
.profile .ui.segment:last-child {
  margin-bottom: 0;
}
.profile .ui.segment:first-child {
  margin-top: 0;
}
.profile .ui.header {
  position: relative;
  z-index: 2;
}
.profile .ui.header.attached {
  border-left: none;
  border-right: none;
  border-radius: 0;
  box-shadow: 0px 3px 6px -1px rgba(0, 0, 0, 0.5);
  margin: 0;
}
.profile .ui.header.attached.top:not(.bottom) {
  border-top: none;
}
.profile .ui.header.attached.bottom:not(.top) {
  border-bottom: none;
}
.profile .promo-block .ui.segment {
  position: relative;
  padding: 0;
  box-shadow: none;
}
.profile .promo-block .promo-header {
  position: relative;
  height: 220px;
  background-position: center;
  background-size: cover;
}
.profile .promo-block .promo-header .ui.header {
  position: absolute;
  left: 200px;
  bottom: 20px;
  color: #fff;
}
.profile .promo-block .ui.menu {
  position: relative;
  z-index: 2;
  border-radius: 0;
  border: none;
  box-shadow: 0px 3px 6px -1px rgba(0, 0, 0, 0.5);
}
.profile .promo-block .ui.menu .item.photo {
  position: relative;
  width: 200px;
  border-right: none;
}
.profile .promo-block .ui.menu .item.photo .image-wrapper {
  position: absolute;
  left: 50%;
  bottom: 17px;
  width: 160px;
  height: 160px;
  min-width: 160px;
  min-height: 160px;
  margin-left: -80px;
  overflow: hidden;
  border-radius: 50%;
  box-shadow: 0px 3px 6px -1px rgba(0, 0, 0, 0.5);
}
.profile .promo-block .ui.menu .item.photo .image-wrapper img {
  height: 100%;
}
.profile .promo-block .ui.menu .item.photo .icons {
  position: absolute;
  bottom: 24px;
  left: 50%;
  margin-left: 38px;
}
.profile .map-block .ui.segment {
  padding: 0;
}
.profile .map-block .ui.segment img {
  display: block;
  max-width: 100%;
}
.profile .photos-box .ui.segment {
  overflow: hidden;
}
.profile .photos-box .image-wrapper {
  width: 33%;
  height: 125px;
  float: left;
  margin-right: 0.5%;
  margin-bottom: 0.5%;
  background-position: center;
  background-size: cover;
}
.profile .photos-box .image-wrapper:nth-child(3n) {
  margin-right: 0;
}
.profile .photos-box .image-wrapper img {
  display: none;
}
.profile .streams-box .ui.segment {
  overflow: hidden;
}
.profile .streams-box .video-wrapper {
  width: 49.5%;
  height: 125px;
  float: left;
  margin-right: 1%;
  margin-bottom: 1%;
}
.profile .streams-box .video-wrapper:first-child {
  width: 100%;
  height: 235px;
  margin-right: 0;
}
.profile .streams-box .video-wrapper:last-child {
  margin-right: 0;
}
.profile .streams-box .video-wrapper img {
  width: 100%;
  height: 100%;
}
